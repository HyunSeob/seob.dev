{
  "name": "seob.dev",
  "reason": { "react-jsx": 3 },
  "bs-dependencies": ["reason-react", "bs-css", "bs-css-emotion"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    },
    {
      "module": "commonjs",
      "in-source": false
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript"
  },
  "ppx-flags": [
    [
      "@reasonml-community/graphql-ppx/ppx",
      "-template-tag=graphql",
      "-fragment-in-query=exclude"
    ]
  ]
}
